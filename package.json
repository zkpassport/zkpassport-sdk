{
  "name": "@zkpassport/sdk",
  "version": "0.2.6",
  "description": "Privacy-preserving identity verification using passports and ID cards",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "type": "module",
  "files": [
    "src/",
    "dist/**",
    "tsconfig.json",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "NODE_OPTIONS='--require ts-node/register' NODE_NO_WARNINGS=1 node node_modules/.bin/jest --no-cache --passWithNoTests --runInBand **/*.test.ts",
    "test:debug": "NODE_OPTIONS='--require ts-node/register' NODE_NO_WARNINGS=1 node --inspect-brk=0.0.0.0 node_modules/.bin/jest --no-cache --passWithNoTests --runInBand **/*.test.ts",
    "deploy": "npm publish --access public"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.9",
    "@types/node-gzip": "^1.1.3",
    "@types/ws": "^8.5.12",
    "jest": "^29.7.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@aztec/bb.js": "^0.67.0",
    "@noble/ciphers": "^1.2.1",
    "@noble/secp256k1": "^2.2.3",
    "@zkpassport/utils": "^0.2.16",
    "buffer": "^6.0.3",
    "i18n-iso-countries": "^7.12.0",
    "node-gzip": "^1.1.2",
    "ws": "^8.18.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
